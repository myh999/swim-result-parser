{
    "name": "swim-result-parser",
    "version": "1.0.0",
    "description": "Swim result parser",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc -p ./tsconfig.json && npm run copy-static-assets",
        "watch": "tsc -p ./tsconfig.json -w",
        "test": "jest -i --no-cache --config ./jest.json",
        "lint": "eslint ./src ./tests ./analysis --ext .ts",
        "clearlog": "rimraf -r ./log/*",
        "psychsheet": "jest -t 'full-psych-sheet-analysis' --no-cache --config ./jest-analysis.json",
        "clearanalysis": "rimraf -r ./analysis/*output*",
        "serve": "node dist/server.js",
        "start": "num run serve",
        "copy-static-assets": "ts-node copy-static-assets.ts"
    },
    "files": [
        "dist"
    ],
    "author": "Max Hao",
    "license": "MIT",
    "dependencies": {
        "express": "^4.17.1",
        "multer": "^1.4.2",
        "node-fetch": "^2.6.1",
        "pdfjs-dist": "^2.6.347"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/multer": "^1.4.5",
        "@types/node": "^14.14.28",
        "@types/node-fetch": "^2.5.8",
        "@types/shelljs": "^0.8.8",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "coveralls": "^3.1.0",
        "eslint": "^7.20.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.4",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.5"
    }
}
